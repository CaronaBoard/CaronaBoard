{
  "name": "caronaboard",
  "version": "1.0.0",
  "description": "Social ride network",
  "main": "index.js",
  "scripts": {
    "start": "yarn generate-html && DEBUG=true webpack-dev-server --hot",
    "start:dist": "http-server build",
    "build": "rm -r ./build; yarn generate-html && webpack -p --progress && sw-precache --config=sw-precache-config.js",
    "build:analyze": "yarn build && open build/stats.html",
    "generate-html": "mkdir -p build && elm-static-html -c elm-static-html.json",
    "test": "elm-test",
    "deploy": "./deploy.sh",
    "deploy:now": "yarn build && now build",
    "install": "elm package install -y && cd tests && elm package install -y"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaronaBoard/caronaboard.git"
  },
  "author": "CaronaBoard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CaronaBoard/caronaboard/issues"
  },
  "homepage": "https://github.com/CaronaBoard/caronaboard#readme",
  "dependencies": {
    "fg-loadcss": "^1.3.1",
    "firebase": "^3.5.3",
    "http-server": "^0.9.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "elm": "^0.18.0",
    "elm-css-webpack-loader": "^3.1.0",
    "elm-hot-loader": "^0.5.4",
    "elm-static-html": "^1.3.0",
    "elm-test": "0.18.2",
    "elm-webpack-loader": "^4.3.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "now": "^4.2.2",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.16.1",
    "sw-precache": "^5.1.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
